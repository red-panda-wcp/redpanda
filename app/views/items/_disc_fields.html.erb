<div class="nested-fields">
<h3>Disc追加</h3>
  <div class="field">
    ディスク番号　　<%= f.text_field :disc_name, placeholder: "例：１" %>
    <%= link_to_remove_association "❎", f %>
  </div>

  <div class="field">
    <%= f.fields_for :songs do |song| %>
      <%= render 'song_fields', f: song %>
    <%end%>
    <div class="links">
      <%= link_to_add_association "[ 曲追加 ]", f, :songs %>
    </div>
  </div>
</div>